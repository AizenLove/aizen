{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:vite": "vite",
    "dev:npm-watch": "npm run npm-watch",
    "dev:type-predicates-generator": "type-predicates-generator -f 'src/types/**/*.ts' -o 'src/utils/predicates.ts' -a -w --default-array-check-option first -c --whitelist",
    "dev": "run-p 'dev:*'",
    "predev": "yarn apigen",
    "apigen": "./bin/generate_api.sh",
    "typecheck": "tsc -p . --noEmit",
    "lint:eslint": "eslint src --cache --ext .ts,.tsx",
    "lint:stylelint": "stylelint --cache 'src/styles/**/*.{css,scss}'",
    "lint:prettier": "prettier '**/*.{ts,tsx,js,json,css,scss,yml,yaml}' --check",
    "lint": "run-p 'lint:*'",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:stylelint": "yarn lint:stylelint --fix",
    "fix:prettier": "prettier '**/*.{ts,tsx,js,json,css,scss,yml,yaml}' --write",
    "fix": "run-p 'fix:*'",
    "npm-watch": "npm-watch",
    "prebuild": "yarn typecheck",
    "build": "vite build",
    "preview": "vite preview"
  },
  "watch": {
    "apigen": {
      "patterns": "../server/python/aizen",
      "extensions": [
        ".py"
      ],
      "tasks": [
        "apigen"
      ]
    }
  },
  "dependencies": {
    "@aspida/axios": "^1.9.1",
    "@aspida/swr": "^1.9.1",
    "@tanstack/react-location": "^3.7.4",
    "aspida": "^1.9.1",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "rambda": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.29.0",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.4",
    "cssnano": "^5.1.7",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.29.4",
    "modern-css-reset": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "openapi2aspida": "^0.19.0",
    "postcss": "^8.4.12",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.3",
    "prettier": "^2.6.1",
    "recoil": "^0.7.0",
    "sass": "^1.49.11",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.2.0",
    "type-predicates-generator": "^0.3.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.0"
  },
  "packageManager": "yarn@3.0.0"
}
